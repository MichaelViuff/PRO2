<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View">
   <children>
      <HBox prefHeight="27.0" prefWidth="600.0">
         <children>
            <Label prefHeight="17.0" prefWidth="117.0" text="Input" />
            <TextField fx:id="inputTextField" />
         </children>
      </HBox>
      <HBox layoutX="10.0" layoutY="10.0" prefHeight="27.0" prefWidth="600.0">
         <children>
            <Label prefHeight="17.0" prefWidth="117.0" text="Output" />
            <TextField fx:id="outputTextField" />
         </children>
      </HBox>
      <Button mnemonicParsing="false" onAction="#onConvertButtonPressed" text="Convert" />
   </children>
</VBox>
